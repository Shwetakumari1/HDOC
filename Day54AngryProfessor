 #include <bits/stdc++.h>

using namespace std;

int main()
{
    int t; cin >> t;
    while (t--) {
        int n, k, sum = 0;
        cin >> n >> k;
        while (n--) {
            int x; cin >> x;
            if (x <= 0) sum++;
        }
        cout << (sum < k ? "YES\n" : "NO\n");
    }
    return 0;
}

