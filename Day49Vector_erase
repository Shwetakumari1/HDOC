int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    vector<int>v;
    int size;
    cin>>size;
    int k;
    for(int i=1;i<=size;i++)
    {
   
     cin>>k;
        v.push_back(k);
    }
    int x,a,b;
    cin>> x;
    cin>> a >> b;
    v.erase(v.begin()+x-1);
    int sum=0;
    v.erase(v.begin()+a-1,v.begin()+b-1);
    cout<<v.size()<<"\n";
    for(int i=0;i<v.size();i++)
        {
             cout<<v[i]<<" ";
        }
    return 0;
}
